{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "EXPO_PUBLIC_SUPABASE_ANON_KEY",
        "EXPO_PUBLIC_API_BASE_URL": "EXPO_PUBLIC_API_BASE_URL",
        "EXPO_PUBLIC_WS_URL": "EXPO_PUBLIC_WS_URL",
        "EXPO_PUBLIC_TERMS_URL": "EXPO_PUBLIC_TERMS_URL",
        "EXPO_PUBLIC_IMAGE_BASE_URL": "EXPO_PUBLIC_IMAGE_BASE_URL",
        "EXPO_PUBLIC_PROJECT_ID": "EXPO_PUBLIC_PROJECT_ID"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}