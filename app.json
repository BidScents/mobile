{
  "expo": {
    "name": "BidScents",
    "slug": "bidscents",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-light.png",
    "scheme": "com.bidscents.mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "buildNumber": "1",
      "bundleIdentifier": "com.bidscents.mobile",
      "icon": {
        "dark": "./assets/icons/ios-dark.png",
        "light": "./assets/icons/ios-light.png",
        "tinted": "./assets/icons/ios-tinted.png"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for messages and listings.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images for messages and listings.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record audio for messages and listings.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "bidscents-oauth",
            "CFBundleURLSchemes": [
              "com.bidscents.mobile"
            ]
          },
          {
            "CFBundleURLName": "bidscents-auth",
            "CFBundleURLSchemes": [
              "com.bidscents.mobile"
            ]
          }
        ],
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "appleTeamId": "BRHM2L7C9T"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bidscents.mobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.bidscents.mobile"
            },
            {
              "scheme": "com.bidscents.mobile",
              "host": "auth"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@stripe/stripe-react-native/app.plugin.js",
        {
          // "merchantIdentifier": "merchant.com.bidscents.app",
          "enableGooglePay": true
        }
      ],
      "expo-notifications",
      [
        "expo-camera",
        {
          "cameraPermission": "This app needs access to your camera to take photos for messages and listings.",
          "microphonePermission": "This app needs access to your microphone to record audio for messages and listings.",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow bidscents to access your photos.",
          "savePhotosPermission": "Allow bidscents to save photos."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "575b76bf-846e-4bc8-95a7-033d66167237"
      }
    },
    "owner": "bidscents"
  }
}
